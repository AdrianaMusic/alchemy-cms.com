<%- content_for :meta_keywords do -%>
<%= 'Alchemy CMS, Showcases, Examples, Sites' -%>
<%- end -%>
<%- content_for :meta_description do -%>
<%= 'Showcases, Examples and Sites build with Alchemy CMS.' -%>
<%- end -%>

<div id="content" class="row">
  <div class="twelve columns">
    <%= render_elements(from_page: showcases_page) %>
    <hr>
  </div>
  <div class="eight columns">
    <ul class="showcases-list">
      <%= render @showcases %>
    </ul>
    <hr>
    <%= paginate(@showcases) %>
  </div>

  <%= render 'showcases/register' %>
</div>
